{
  "version": 2,
  "name": "agentsummary-frontend",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "functions": {
    "api/chat.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "ANTHROPIC_API_KEY": "@anthropic_api_key"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
